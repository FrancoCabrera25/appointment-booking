{
    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "node",
        "request": "launch",
        "name": "Launch NestJS App",
        "program": "${workspaceFolder}/node_modules/.bin/nest",
        "args": ["start"],
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"]
      }
    ]
  }
  